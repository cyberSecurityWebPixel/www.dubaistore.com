(async() => {
  let clickId = getCookie("s2stracking");
  if (!clickId || clickId === '') clickId = "df61c968-dab5-470d-a842-aea77d7294b2";

  const expires = (new Date(Date.now() + 30 * 86400 * 1000)).toUTCString();
  document.cookie = "s2stracking" + '=' + clickId + '; expires=' + expires + 86400 +
      ';path=/';

  const data = {
    url: window.location.href,
    referrer: document.referrer,
    unique_id: clickId,
  };

  await fetch("https://itp.s2stracking.com" + "/j2/tracck", {
    method: 'POST',
    body: JSON.stringify(data),
    headers: {
      'Content-Type': 'application/json',
    },
  });

  function getCookie(cname) {
    const name = cname + '=';
    const ca = document.cookie.split(';');
    for (let i = 0; i < ca.length; i++) {
      let c = ca[i];
      while (c.charAt(0) === ' ') {
        c = c.substring(1);
      }
      if (c.indexOf(name) === 0) {
        return c.substring(name.length, c.length);
      }
    }
    return '';
  }
})();